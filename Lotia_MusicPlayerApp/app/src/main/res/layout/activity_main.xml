<?xml version="1.0" encoding="utf-8"?>

<GridLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/GenreView"
    style="@style/musicCategoryGroupViewStyle"
    android:columnCount="2"
    android:rowCount="2"
    tools:context=".MainActivity">

    <ImageView
        android:id="@+id/gospel"
        style="@style/musicCategoryStyle"
        android:src="@drawable/gospel" />

    <ImageView
        android:id="@+id/hipHop"
        style="@style/musicCategoryStyle"
        android:src="@drawable/hiphop" />

    <ImageView
        android:id="@+id/pop"
        style="@style/musicCategoryStyle"
        android:src="@drawable/pop" />

    <ImageView
        android:id="@+id/classical"
        style="@style/musicCategoryStyle"
        android:src="@drawable/classical" />

</GridLayout>