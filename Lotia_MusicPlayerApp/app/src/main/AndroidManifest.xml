<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.mandy.lotiamusicplayer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GospelActivity"
            android:label="Gospel Music" />
        <activity
            android:name=".HipHopActivity"
            android:label="HipHop" />
        <activity
            android:name=".PopActivity"
            android:label="Pop Music" />
        <activity
            android:name=".ClassicalActivity"
            android:label="Classical Music" />
        <activity
            android:name=".NowPlayingActivity"
            android:label="Now Playing">
        </activity>
    </application>

</manifest>